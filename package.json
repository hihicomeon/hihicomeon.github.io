{
	"name": "D20_damageTest",
	"private": true,
	"version": "1.0.0",
	"homepage": "https://hihicomeon.github.io/SilenceeD20/",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:prettier": "prettier {src,mock}/**/*.{html,css,sass,scss,md,less,ts,tsx,js,jsx} --write",
		"test": "jest --coverage",
		"preview": "vite preview",
		"commit": "git-cz",
		"prepare": "husky install",
		"predeploy": "yarn run build", 
		"deploy": "gh-pages -d build"
	},
	"dependencies": {
		"@icon-park/react": "^1.4.2",
		"@reduxjs/toolkit": "^1.9.5",
		"antd": "^5.5.1",
		"antd-img-crop": "^4.12.2",
		"axios": "^1.4.0",
		"dayjs": "^1.11.7",
		"echarts": "^5.4.3",
		"echarts-for-react": "^3.0.2",
		"enable-proxy": "^0.1.2",
		"localforage": "^1.10.0",
		"mathjs": "^11.8.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.7",
		"react-router-dom": "^6.11.2",
		"store": "^2.0.12",
		"styled-components": "^5.3.11",
		"use-debounce": "^9.0.4"
	},
	"devDependencies": {
		"@jest/test-sequencer": "^29.5.0",
		"@storybook/testing-library": "^0.1.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/echarts": "^4.9.18",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.2.5",
		"@types/ramda": "^0.29.2",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/store": "^2.0.2",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"@vitejs/plugin-react-swc": "^3.0.0",
		"babel-jest": "^29.5.0",
		"electron": "^26.2.1",
		"electron-builder": "^24.6.4",
		"eslint": "^8.38.0",
		"eslint-config-alloy": "^5.0.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.3.4",
		"gh-pages": "^6.0.0",
		"git-cz": "^4.9.0",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-transform-stub": "^2.0.0",
		"less": "^4.1.3",
		"path-to-regexp": "^6.2.1",
		"prettier": "^2.8.8",
		"ramda": "^0.29.0",
		"ts-jest": "^29.1.0",
		"typescript": "^5.0.2",
		"vite": "^4.3.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json}": [
			"prettier --write",
			"eslint",
			"git add"
		]
	},
	"build": {
		"appId": "com.my-website.my-app",
		"productName": "D20",
		"copyright": "Copyright Â© 2019 ${author}",
		"mac": {
			"category": "public.app-category.utilities"
		},
		"win": {
			"icon": "./public/loading.jpg",
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64",
						"ia32"
					]
				}
			]
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"files": [
			"dist/**/*",
			"electron/**/*"
		],
		"directories": {
			"buildResources": "assets",
			"output": "dist_electron"
		}
	},
	"main": "electron/electron.js"
}
